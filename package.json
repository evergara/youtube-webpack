{
  "name": "youtube-webpack",
  "version": "1.0.0",
  "description": "Aprendiendo webpack",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "build-entrada-personalizada": "webpack --mode production --entry ./personalizado/src/index.js --output-path ./personalizado/dist",
    "dev-entrada-personalizada": "webpack --mode development --entry ./personalizado/src/index.js --output-path ./personalizado/dist",
    "start": "webpack serve  --mode development --open --port 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evergara/youtube-webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Emerson vergara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evergara/youtube-webpack/issues"
  },
  "homepage": "https://github.com/evergara/youtube-webpack#readme",
  "dependencies": {
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "webpack-dev-server": "^3.11.2"
  }
}
